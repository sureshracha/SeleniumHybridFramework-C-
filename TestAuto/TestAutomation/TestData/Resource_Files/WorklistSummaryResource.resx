  <data name="DBQueryforFIlteredDataAccountsWorklist" xml:space="preserve">
    <value>SELECT 
              count(DISTINCT CAST(wq.PatientVisitID AS varchar(20))+CAST(wq.OWNER AS varchar(10))+CAST(ISNULL(wq.ClaimID, 0) AS varchar(20))+pv.PatientVisitAccount) PatientVisitAccount
                    , 
                     ISNULL(sum(
                    CASE 
                           WHEN q.BalanceType = 1 THEN ft.CurrentAccountBalance
                           WHEN q.BalanceType = 2 THEN ft.TXNCurrentBalance
                           WHEN q.BalanceType = 3 THEN ce.ClaimAmount
                           WHEN q.BalanceType = 4 THEN ft.CaseValue
                           ELSE ft.CurrentAccountBalance
             END), 0.00) CurrentAccountBalance,

             (CASE wq.FlagCode WHEN 'H' then 1 else 0 end) Ishold
             
FROM dbo.tblWorklistQueues wq  WITH (NOLOCK) 
                                    INNER JOIN dbo.tblQueues q WITH (NOLOCK) ON q.SecurityObjectID = wq.SecurityObjectID
                                    LEFT JOIN dbo.tblClaimOfPatientVisit cop1 WITH (NOLOCK) ON wq.PatientVisitID = cop1.PatientVisitID AND cop1.IsMostCurrent = 1
                                    LEFT JOIN dbo.vw_WF_PatientVisitClaimsExtDetails ce ON wq.PatientVisitID = ce.PatientVisitID AND (wq.ClaimID = ce.ClaimID OR wq.ClaimID = cop1.ClaimID) 
LEFT JOIN dbo.tblPatientVisits pv  WITH (NOLOCK)  ON wq.PatientVisitID = pv.PatientVisitID
LEFT JOIN dbo.tblFinancialTotals ft  WITH (NOLOCK)  ON wq.PatientVisitID = ft.PatientVisitID
INNER JOIN tblPatientVisits pvs WITH (NOLOCK) ON wq.PatientVisitID = pvs.PatientVisitID
INNER JOIN (SELECT ur.OrganizationID
FROM dbo.tblUsers u WITH (NOLOCK)
LEFT JOIN dbo.tblUserOrgRights ur WITH (NOLOCK) ON u.UserID = ur.UserID
WHERE u.UserID = 5096
       AND ur.HasRight = 1
       AND ur.OrganizationID IS NOT NULL 
       union SELECT gr.OrganizationID
FROM dbo.tblGroupMembers gm WITH (NOLOCK)
LEFT JOIN tblGroupOrgRights gr WITH (NOLOCK) ON gr.GroupID = gm.GroupID
WHERE gm.UserID = 2909
       AND gr.OrganizationID IS NOT NULL) tor ON tor.OrganizationID = pvs.OrganizationID
WHERE   ( wq.FlagCode &lt;&gt; 'A' OR wq.Owner &lt;&gt; 1)
AND wq.SecurityObjectID = 18247 AND wq.FlagCode &lt;&gt; 'I'
AND wq.ClaimID = 0 
 GROUP BY  (CASE wq.FlagCode WHEN 'H' then 1 else 0 end)</value>
  </data>
